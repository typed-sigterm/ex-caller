{
  "build": {
    "beforeDevCommand": "run-p dev:web",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:6408"
  },
  "bundle": {
    "active": true,
    "targets": ["dmg", "nsis"],
    "windows": {
      "nsis": {
        "languages": ["SimpChinese", "English"]
      },
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "publisher": "Typed SIGTERM",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "./installed"
    ]
  },
  "productName": "ExCaller",
  "version": "1.1.1",
  "identifier": "top.by-ts.ex-caller",
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ExCaller",
        "width": 800,
        "height": 600,
        "center": true
      }
    ]
  }
}
